<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="youtuattrs">
        <attr name="cameraPosition">
            <flag name="front" value="0" />
            <flag name="back" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>



    <declare-styleable name="WaterMarkText">
        <attr name="degree" format="integer" />
    </declare-styleable>
</resources>
